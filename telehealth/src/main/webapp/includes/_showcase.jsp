<!--contact-->
<section id="contact">
	<div class="container contact pt-5 pb-5 ">

		<div class="container px-4 px-lg-5 mb-5  " >

			<h1 class="center pt-5 white" data-aos="fade-down" data-aos-duration='1000'>Contact Us</h1>
			<div class="row gx-4  gx-lg-5 " data-aos="slide-right" data-aos-duration='1000'>
				<div class="col-lg-6 col-md-12 mb-3 mb-md-0 mt-5 ">
					<div class="px-5 white "></div>
				</div>
				<div class="col-lg-6 col-md-12 mb-3 mb-md-0 mt-5  ">
					<div class="contactForm mt-5">
						<form id="send-message" action="message">
							<h2>Send Message</h2>
							<div class="inputBox">
								<input type="text" name="fullname" required="required"> <span>Full
									Name</span>
							</div>
							<div class="inputBox">
								<input type="text" name="email" required="required"> <span>Email</span>
							</div>
							<div class="inputBox">
								<textarea name="message" required="required" id="text_a"></textarea>
								<span>Type your Message...</span>
							</div>
							<div class="inputBox">
								<div class="col-auto">
									<button type="submit" class="btn btn-login">Send</button>
								</div>
							</div>
						</form>
					</div>

				</div>
			</div>
		</div>
	</div>
</section>